{
    "manifest_version": 3,
    "name": "SAPAI extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "background/background.js"
    }
  }